{
  "name": "cdnjs.dev",
  "version": "1.0.0",
  "description": "The new cdnjs website, using Vue & VueRouter",
  "main": "index.js",
  "scripts": {
    "dev": "npm run dev:static && npm run dev:parcel & npm run dev:server",
    "dev:static": "copyfiles --up 2 src/static/{*,**/*} dev",
    "dev:parcel": "parcel watch src/index.html --out-dir dev",
    "dev:server": "node src/server/app.js",
    "build": "npm run build:clean && npm run build:static && npm run build:parcel",
    "build:clean": "rm -rf dist",
    "build:static": "copyfiles --up 2 ./src/static/{*,**/*} dist",
    "build:parcel": "parcel build src/index.html --out-dir dist",
    "test": "npm run test:eslint && npm run test:scss",
    "test:eslint": "eslint 'src/**/*.{js,vue}'",
    "test:eslint:fix": "npm run test:eslint -- --fix",
    "test:scss": "sass-lint 'src/**/*.scss' --verbose --no-exit --config .sasslintrc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MattIPv4/cdnjs.dev.git"
  },
  "author": "Matt (IPv4) Cowley",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/MattIPv4/cdnjs.dev/issues"
  },
  "homepage": "https://github.com/MattIPv4/cdnjs.dev#readme",
  "dependencies": {
    "algoliasearch": "^4.0.3",
    "babel-polyfill": "^6.26.0",
    "instantsearch.css": "^7.4.2",
    "node-fetch": "^2.6.0",
    "parcel-bundler": "^1.12.4",
    "semver-sort": "0.0.4",
    "spdx-license-ids": "^3.0.5",
    "thenby": "^1.3.0",
    "vue": "^2.6.11",
    "vue-clipboard2": "^0.3.1",
    "vue-hot-reload-api": "^2.3.4",
    "vue-instantsearch": "^2.7.0",
    "vue-router": "^3.1.6",
    "vue-select": "^3.4.0",
    "vue-tippy": "^4.4.0"
  },
  "devDependencies": {
    "@vue/component-compiler-utils": "^3.1.1",
    "babel-eslint": "^10.1.0",
    "copyfiles": "^2.2.0",
    "eslint": "^6.8.0",
    "eslint-plugin-vue": "5.2.3",
    "express": "^4.17.1",
    "sass": "^1.26.1",
    "sass-lint": "^1.13.1",
    "vue-eslint-parser": "^7.0.0",
    "vue-template-compiler": "^2.6.11"
  }
}

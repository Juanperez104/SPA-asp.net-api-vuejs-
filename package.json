{
  "name": "cdnjs.dev",
  "version": "1.0.0",
  "description": "The new cdnjs website, using Vue & Nuxt",
  "main": "index.js",
  "scripts": {
    "dev": "NODE_ENV=dev nodemon server/index.js --watch server --watch nuxt.config.js",
    "dev:generate": "NODE_ENV=production SITE_HOST=http://localhost:3000/ nuxt generate && node server/generate.js",
    "dev:generate:noroutes": "NODE_ENV=dev SITE_HOST=http://localhost:3000/ nuxt generate && node server/generate.js",
    "dev:analyze": "NODE_ENV=production SITE_HOST=http://localhost:3000/ nuxt build --analyze",
    "build": "NODE_ENV=production SITE_HOST=https://cdnjs.dev/ nuxt generate",
    "test": "npm run test:eslint && npm run test:scss",
    "test:eslint": "eslint '!(node_modules|.nuxt|dist)/**/*.{js,vue}'",
    "test:eslint:fix": "npm run test:eslint -- --fix",
    "test:scss": "sass-lint '!(node_modules|.nuxt|dist)/**/*.scss' --verbose --no-exit --config .sasslintrc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MattIPv4/cdnjs.dev.git"
  },
  "author": "Matt (IPv4) Cowley",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/MattIPv4/cdnjs.dev/issues"
  },
  "homepage": "https://github.com/MattIPv4/cdnjs.dev#readme",
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.28",
    "@fortawesome/free-brands-svg-icons": "^5.13.0",
    "@fortawesome/free-regular-svg-icons": "^5.13.0",
    "@fortawesome/free-solid-svg-icons": "^5.13.0",
    "@fortawesome/vue-fontawesome": "^0.1.9",
    "@nuxtjs/sitemap": "^2.0.1",
    "algoliasearch": "^4.0.3",
    "babel-polyfill": "^6.26.0",
    "chunk": "0.0.2",
    "glob-to-regexp": "^0.4.1",
    "instantsearch.css": "^7.4.2",
    "markdown-it": "^10.0.0",
    "node-fetch": "^2.6.0",
    "nuxt": "^2.11.0",
    "prismjs": "^1.19.0",
    "semver-sort": "0.0.4",
    "spdx-license-ids": "^3.0.5",
    "thenby": "^1.3.0",
    "tlite": "^0.1.9",
    "typeface-ubuntu": "0.0.65",
    "url-parse": "^1.4.7",
    "vue": "^2.6.11",
    "vue-client-only": "^2.0.0",
    "vue-clipboard2": "^0.3.1",
    "vue-hot-reload-api": "^2.3.4",
    "vue-instantsearch": "^3.0.0-beta.0",
    "vue-prism-component": "^1.1.1",
    "vue-router": "^3.1.6",
    "vue-select": "^3.4.0"
  },
  "devDependencies": {
    "@nuxtjs/eslint-config": "^2.0.2",
    "babel-eslint": "^10.1.0",
    "copyfiles": "^2.2.0",
    "eslint": "^6.8.0",
    "eslint-plugin-nuxt": "^0.5.1",
    "eslint-plugin-vue": "5.2.3",
    "express": "^4.17.1",
    "node-sass": "^4.13.1",
    "nodemon": "^2.0.2",
    "sass-lint": "^1.13.1",
    "sass-loader": "^8.0.2",
    "vue-eslint-parser": "^7.0.0"
  }
}
